
@model IEnumerable<API.Models.Item_MasterVO>

@{
    ViewBag.Title = "Index";
}

<!-- USERS LIST -->
<div class="box box-danger">
    <div class="box-header with-border">
        <h3 class="box-title">Latest Members</h3>

        <div class="box-tools pull-right">
            <span class="label label-danger">@Model.Count()</span>
            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                <i class="fa fa-minus"></i>
            </button>
            <button type="button" class="btn btn-box-tool" data-widget="remove">
                <i class="fa fa-times"></i>
            </button>
        </div>
    </div>
    <!-- /.box-header -->
    <div class="box-body no-padding">
        <ul class="users-list clearfix">
     
            @foreach (var item in Model)
            {

                <li>
                    <img src="@Html.DisplayFor(modelItem => item.Lead_Time)" alt="User Image">
                    <a class="users-list-name" href="#">상세정보</a>
                    <span class="users-list-date">   @Html.DisplayFor(modelItem => item.Item_Name)</span>
                </li>

            }
        </ul>
        <!-- /.users-list -->
    </div>
    <!-- /.box-body -->
    <div class="box-footer text-center">
        <a href="javascript:void(0)" class="uppercase">View All Users</a>
    </div>
    <!-- /.box-footer -->
</div>

